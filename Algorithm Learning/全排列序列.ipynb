   "source": [
    "def permute(nums):\n",
    "    e = []\n",
    "    if (len(nums)==1):\n",
    "        return [nums]\n",
    "    for i in range(len(nums)):\n",
    "        q = permute(nums[:i]+nums[i+1:])  #( ,i]+[i+1,end):每次将i提出来加在序列最后\n",
    "        for c in q:\n",
    "            e.append([nums[i]] + c)\n",
    "    return e\n",
    "print(permute(nums=[1,2,3]))"
   ]
  }
 ],
